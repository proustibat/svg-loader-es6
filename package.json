{
  "name": "svg-loader-es6",
  "version": "1.1.1",
  "description": "SVG Loader Vanilla ES6",
  "keywords": [
    "svg",
    "loader",
    "es6",
    "vanilla",
    "js",
    "vanilla js",
    "vanilla es6",
    "svg animation"
  ],
  "main": "src/svg-loader-es6.js",
  "repository": "git@github.com:proustibat/svg-loader-es6.git",
  "author": "Proustibat <proustibat@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "jsdoc": "./node_modules/.bin/documentation build src/svg-loader-es6.js -f html -o jsdoc -g && open ./jsdoc/index.html",
    "esdoc": "./node_modules/.bin/esdoc -c ./esdoc.json && open ./esdoc/index.html",
    "lint:js": "eslint src --ext .js -c .eslintrc.js --fix",
    "postversion": "git push origin master --tags"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "documentation": "6.3.2",
    "esdoc": "1.0.4",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "4.18.0",
    "eslint-config-eslint": "4.0.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-node": "6.0.0",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
