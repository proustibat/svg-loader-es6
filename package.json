{
  "name": "svg-loader-es6",
  "version": "1.1.1",
  "description": "SVG Loader Vanilla ES6",
  "keywords": [
    "svg",
    "loader",
    "es6",
    "vanilla",
    "js",
    "vanilla js",
    "vanilla es6",
    "svg animation"
  ],
  "main": "src/svg-loader-es6.js",
  "repository": "git@github.com:proustibat/svg-loader-es6.git",
  "author": "Proustibat <proustibat@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "jsdoc": "./node_modules/.bin/documentation build src/svg-loader-es6.js -f html -o jsdoc -g && open ./jsdoc/index.html",
    "esdoc": "./node_modules/.bin/esdoc -c ./esdoc.json && open ./esdoc/index.html",
    "lint:js": "eslint src --fix",
    "postversion": "git push origin master --tags"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.15.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "documentation": "5.4.0",
    "esdoc": "1.0.4",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "9.15.0",
    "eslint-config-eslint": "11.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.14.0",
    "eslint-plugin-promise": "7.2.0",
    "eslint-webpack-plugin": "4.2.0",
    "globals": "15.12.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
